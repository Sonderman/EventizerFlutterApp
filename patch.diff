+  Widget build(BuildContext context) {
+    switch(authStatus){
+      case AuthStatus.NotSignetIn:
+        return LoginPage(auth: widget.auth,onSignedIn: _signedIn,);
+
+      case AuthStatus.SignedIn :
+        return HomePage(
+          auth: widget.auth,
+          onSignedOut: _signedOut,
+        );
+    }
+  }
+}
